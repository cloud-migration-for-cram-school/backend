{
  "version": 1,
  "paths": {
    "/search/subjects/{sheetId}": {
      "name": "/search/subjects/{sheetId}",
      "lambdaFunction": "getSubjects",
      "permissions": {
        "setting": "open"
      }
    },
    "/search": {
      "name": "/search",
      "lambdaFunction": "getSearchInfo",
      "permissions": {
        "setting": "open"
      }
    },
    "/search/subjects/reports/{sheetId}/{subjectsId}": {
      "name": "/search/subjects/reports/{sheetId}/{subjectsId}",
      "lambdaFunction": "userInfoExp",
      "permissions": {
        "setting": "open"
      }
    },
    "/submit/report/{sheetId}/{subjectsId}": {
      "name": "/submit/report/{sheetId}/{subjectsId}",
      "lambdaFunction": "submitReport",
      "permissions": {
        "setting": "open"
      }
    }
  }
}